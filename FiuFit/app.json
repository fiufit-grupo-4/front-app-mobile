{
  "expo": {
    "name": "FiuFit",
    "slug": "FiuFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fiticon.png",
    "userInterfaceStyle": "light",
    "scheme":"fiufit",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fiticon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "fiufit.app.mobile",
      "versionCode": 1,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.gms.permission.ACTIVITY_RECOGNITION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/fiufiticon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FiuFit to use your location."
        }
      ],
      ["@react-native-google-signin/google-signin"]
    ],
    "extra": {
      "eas": {
        "projectId": "328698f9-d8bd-4426-86bc-a4e44658a679"
      }
    }
  }
}
